<div class="containar">
  <div class="row">
    
    <h4><%= @customer.full_name %>さんの会員情報編集</h4>
    
    <table>
      <tr>
        <th>会員ID</th>
        <td><%= @customer.id %></td>
      </tr>
      
      <tr>
        <th>氏名</th>
        <td><%= f.text_field :last_name %></td>
        <td><%= f.text_field :first_name %></td>
      </tr>
      
      <tr>
        <th>フリガナ</th>
        <td><%= f.text_field :last_neme_kana %></td>
        <td><%= f.text_field :first_name_kana %></td>
      </tr>
      
      <tr>
        <th>郵便番号</th>
        <td><%= f.text_field :postal_code %></td>
      </tr>
      
      <tr>
        <th>住所</th>
        <td><%= f.text_field :address %></td>
      </tr>
      
      <tr>
        <th>電話番号</th>
        <td><%= f.text_field :telephone_number %></td>
      </tr>
      
      <tr>
        <th>メールアドレス</th>
        <td><%= f.text_field :email %></td>
      </tr>
      
      <tr>
        <th>会員ステータス</th>
        <td></td>
      </tr>
    </table>
    
  </div>
</div>