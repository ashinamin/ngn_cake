<div class="container">
    <!--<div class="row"></div>-->
    <div class="row">
      <div class='col-lg-3'>
      <p>ジャンル検索が入ります</p>
      </div>
      <div class='col-lg-9'>
        <h4><strong>商品一覧</strong>(全<%= items.count %>件)</h4>
        <div class="row">
          <% @items.each do |item| %>
              <div class="col-lg-3">
                <% link_to public_item(item) %>
                <%= image_tag item.item_image, size: '80x50' %>
                <%= item.name %>
                ¥<%= item.add_tax_price.to_s(:dalimited) %>
              </div>
          <% end %>
        </div>
      </div>
      <div class="row">
        <%= paginate @items %>
      </div>
    </div>
</div>